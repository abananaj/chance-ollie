{
  "version": 3,
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "background": {
      "backgroundImage": true,
      "backgroundSize": true
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "dimensions": {
      "aspectRatio": true,
      "minHeight": true
    },
    "layout": {
      "allowCustomContentAndWideSize": true,
      "allowEditing": true,
      "contentSize": "1200px",
      "wideSize": "100%"
    },
    "lightbox": {
      "allowEditing": true,
      "enabled": true
    },
    "position": {
      "sticky": true
    },
    "shadow": {
      "defaultPresets": true
    },
    "spacing": {
      "blockGap": true,
      "customSpacingSize": true,
      "margin": true,
      "padding": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "color": {
      "background": true,
      "button": true,
      "caption": true,
      "heading": true,
      "link": true,
      "text": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultDuotone": true,
      "defaultGradients": true,
      "defaultPalette": false,
      "palette": [
        { "name": "Base", "slug": "base", "color": "#C4381A" },
        { "name": "Complementary", "slug": "comp", "color": "#D99A4E" },
        { "name": "Contrast", "slug": "contrast", "color": "#27524C" },
        { "name": "Accent", "slug": "accent", "color": "#E322A9" },
        { "name": "Highlight", "slug": "highlight", "color": "#0FF207" },
        { "name": "Background", "slug": "bg", "color": "#05051A" },
        { "name": "Text", "slug": "txt", "color": "#FFFAFD" },
        { "name": "Background Alt", "slug": "bgalt", "color": "#27524C" },
        { "name": "Text Alt", "slug": "txtalt", "color": "#E1EFF2" },
        { "name": "Link", "slug": "link", "color": "#0FF207" },
        { "name": "Light", "slug": "light", "color": "#bafaf9" },
        { "name": "Dark", "slug": "dark", "color": "#03030C" },
        {
          "color": "#E322A9",
          "slug": "hollywood-cerise",
          "name": "Hollywood Cerise"
        },
        { "color": "#0FF207", "slug": "harlequin", "name": "Harlequin" },
        { "color": "#29F2F2", "slug": "aqua", "name": "Aqua" },
        {
          "color": "#144FC4",
          "slug": "cerulean-blue",
          "name": "Cerulean Blue"
        },
        { "color": "#000F5C", "slug": "lucky-point", "name": "Lucky Point" },
        { "color": "#05051A", "slug": "ebony", "name": "Ebony" },
        { "color": "#24041B", "slug": "revolver", "name": "Revolver" },
        { "color": "#cbfdbc", "slug": "gossip", "name": "Gossip" },
        { "color": "#FFFAFD", "slug": "hint-of-red", "name": "Hint of Red" },
        { "color": "#bafaf9", "slug": "mint-tulip", "name": "Mint Tulip" },
        { "color": "#03030C", "slug": "jaguar", "name": "Jaguar" }
      ]
    },
    "custom": {
      "border": {
        "color": "#03030C",
        "width": "2px",
        "style": "solid",
        "radius": "25px"
      },
      "btn": {
        "bg": "#BB6869",
        "bgHov": "#F2CECF",
        "txt": "#fff",
        "txtHov": "#000000",
        "trans": "500ms"
      },
      "fontWeight": {
        "thin": 100,
        "extra-light": 200,
        "light": 300,
        "regular": 400,
        "medium": 500,
        "semi-bold": 600,
        "bold": 700,
        "extra-bold": 800,
        "black": 900
      },
      "lineHeight": {
        "heading": 1.5,
        "none": 0,
        "single": 1,
        "tight": 1.1,
        "snug": 1.2,
        "base": 1.3,
        "body": 1.3,
        "normal": 1.3,
        "relaxed": 1.618,
        "loose": 2.62
      },
      "transition": {
        "heading": "500ms"
      },
      "fontVarSet": {
        "caption": "'wdth' 100, 'wght' 1, 'STCH' 1;",
        "nav": "'wdth' 100, 'wght' 1, 'STCH' 1;",
        "navHov": "'wdth' 150, 'wght' 1, 'STCH' 1;",
        "btn": "'wdth' 100, 'wght' 1, 'STCH' 1;",
        "btnHov": "'wdth' 150, 'wght' 1, 'STCH' 1;",
        "body": "'wdth' 100, 'wght' 1, 'STCH' 1;",
        "display": "'wdth' 100, 'wght' 1, 'STCH' 1;",
        "h1": "'wdth' 100, 'wght' 1, 'STCH' 1;",
        "h2": "'wdth' 100, 'wght' 1, 'STCH' 1;",
        "h3": "'wdth' 100, 'wght' 1, 'STCH' 1;",
        "h4": "'wdth' 100, 'wght' 1, 'STCH' 1;",
        "h5": "'wdth' 100, 'wght' 1, 'STCH' 1;",
        "h6": "'wdth' 100, 'wght' 1, 'STCH' 1;"
      }
    },
    "typography": {
      "customFontSize": true,
      "defaultFontSizes": false,
      "dropCap": true,
      "fluid": true,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "lineHeight": true,
      "textAlign": true,
      "textColumns": true,
      "textDecoration": true,
      "textTransform": true,
      "writingMode": true,
      "fontFamilies": [
        {
          "name": "Inconsolata",
          "slug": "inconsolata",
          "fontFamily": "'inconsolata',monospace"
        },
        {
          "name": "Gabarito",
          "slug": "gabarito",
          "fontFamily": "'gabarito',sans-serif"
        },
        {
          "name": "Space Grotesk",
          "slug": "space-grotesk",
          "fontFamily": "'space-grotesk',sans-serif"
        },
        {
          "name": "Crimson Pro",
          "slug": "crimson-pro",
          "fontFamily": "'crimson-pro',serif"
        },

        {
          "name": "Serif",
          "slug": "serif",
          "fontFamily": "'crimson-pro',serif"
        },

        {
          "name": "Headings",
          "slug": "headings",
          "fontFamily": "'gabarito',sans-serif"
        },
        {
          "name": "Headings Alt",
          "slug": "headings-alt",
          "fontFamily": "'space-grotesk',sans-serif"
        },
        {
          "name": "Body",
          "slug": "body",
          "fontFamily": "'inconsolata',monospace"
        },
        {
          "name": "Display",
          "slug": "display",
          "fontFamily": "'space-grotesk',sans-serif"
        },
        {
          "name": "Button",
          "slug": "btn",
          "fontFamily": "'inconsolata',monospace"
        },
        {
          "name": "Nav",
          "slug": "nav",
          "fontFamily": "'inconsolata',monospace"
        },
        {
          "name": "Caption",
          "slug": "caption",
          "fontFamily": "'space-grotesk',sans-serif"
        }
      ],
      "fontSizes": [
        {
          "slug": "h1",
          "size": "4.26rem",
          "name": "Heading 1"
        },
        {
          "slug": "h2",
          "size": "2.62rem",
          "name": "Heading 2"
        },
        {
          "slug": "h3",
          "size": "1.618rem",
          "name": "Heading 3"
        },
        {
          "slug": "h4",
          "size": "1.33rem",
          "name": "Heading 4"
        },
        {
          "slug": "h5",
          "size": "1.25rem",
          "name": "Heading 5"
        },
        {
          "slug": "h6",
          "size": "1.125rem",
          "name": "Heading 6"
        },
        {
          "slug": "xs",
          "size": ".56rem",
          "name": "Extra Small"
        },
        {
          "slug": "small",
          "size": ".833rem",
          "name": "Small"
        },
        {
          "slug": "base",
          "size": "calc(15px + 0.390625vw)",
          "name": "Base"
        },
        {
          "slug": "body",
          "size": "calc(15px + 0.390625vw)",
          "name": "Body"
        },
        {
          "slug": "medium",
          "size": "calc(15px + 0.390625vw)",
          "name": "Medium"
        },
        {
          "slug": "large",
          "size": "1.33rem",
          "name": "Large"
        },
        {
          "slug": "x-large",
          "size": "1.618rem",
          "name": "XL"
        },
        {
          "slug": "xx-large",
          "size": "2.62rem",
          "name": "XXL"
        },
        {
          "slug": "xxx-large",
          "size": "4.26rem",
          "name": "XXXL"
        }
      ]
    }
  },
  "styles": {
    "border": {
      "color": "var(--wp--custom--border--color)",

      "style": "var(--wp--custom--border--style)",
      "width": "var(--wp--custom--border--width)",
      "radius": "var(--wp--custom--border--radius)"
    },
    "color": {
      "background": "var(--wp--preset--color--bg)",
      "text": "var(--wp--preset--color--text)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--body)",
      "fontSize": "var(--wp--preset--font-size--body)",
      "fontWeight": "var(--wp--custom--font-weight--light)",
      "lineHeight": "var(--wp--custom--line-height--body)"
    }
  }
}
